{
  "extends": "tslint:latest",
  "rules": {
    "member-access": [
      false
    ],
    "arrow-parens": false,
    "interface-name": [
      false
    ],
    "ordered-imports": [
      false
    ],
    "object-literal-sort-keys": false,
    "member-ordering": [
      false
    ],
    "trailing-comma": [
      true,
      {
        "multiline": "always",
        "singleline": "never"
      }
    ],
    "object-literal-key-quotes": [
      true,
      "as-needed"
    ],
    "no-unused-expression": true,
    "no-unused-variable": [
      true
    ],
    "linebreak-style": [
      true,
      "LF"
    ],
    "no-trailing-whitespace": true,
    "object-literal-shorthand": true,
    "semicolon": [
      true,
      "always"
    ],
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-module",
      "check-separator",
      "check-type",
      "check-typecast"
    ]
  }
}
